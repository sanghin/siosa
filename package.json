{
  "name": "siosa",
  "version": "0.0.1",
  "description": "Path Of Exile build library",
  "bugs": "https://github.com/sanghin/siosa/issues",
  "repository": "github:sanghin/siosa",
  "license": "MIT",
  "keywords": [
    "path of exile",
    "poe",
    "search",
    "tool",
    "library",
    "build"
  ],
  "author": "Sanghin",
  "contributors": [
    "Luc Tribolet <luc.tribolet@gmail.com> (https://tribolet.fr)"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "npx eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "test": "NODE_ENV=testing jest",
    "test-coverage": "NODE_ENV=testing jest --coverage --coverageDirectory=output/coverage/jest",
    "knex": "$(yarn bin)/knex --knexfile ./server/database/knexfile.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "scripts/commitmsg $HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": "^13.5.0",
    "yarn": "^1.21.0"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "bookshelf": "^1.0.1",
    "dotenv": "^8.2.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest-transform-stub": "^2.0.0",
    "knex": "^0.20.4",
    "next": "^9.1.5",
    "node-fetch": "^2.6.0",
    "pg": "^7.14.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.8.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.18",
    "@types/react": "^16.9.16",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gitmoji-cli": "^3.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "object.fromentries": "^2.0.2",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.1",
    "typescript": "^3.7.4"
  }
}
